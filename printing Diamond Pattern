for i in range(7):
    for j in range(7):
        if(i==0 and j==3)or(i==1 and j%2==0 and j%3!=0)or(i==2 and j%2==1)or(i==3 and j%2==0):
           print("*",end='')
        elif(i==4 and j%2==1)or(i==5 and j%2==0 and j%3!=0)or(i==6 and j==3):
           print("*",end='')
        else:
           print(end=' ')
    print()
