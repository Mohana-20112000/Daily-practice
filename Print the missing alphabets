user_input=input()
s=''
if user_input.islower() and user_input.isalnum():
    for j in user_input:
        if j in '0123456789':
            user_input=user_input.replace(j,'')
    for i in 'abcdefghijklmnopqrstuvwxyz':
        if  i in user_input:
            continue
        s=s+i
        
print(s)
